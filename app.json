{
  "id": "app.picnic",
  "version": "2.1.2",
  "compatibility": ">=2.0.0",
  "sdk": 2,
  "name": {
    "en": "Picnic"
  },
  "description": {
    "en": "Doing your groceries via Picnic."
  },
  "category": [
    "internet"
  ],
  "brandColor": "#e40613",
  "permissions": [
    "homey:manager:speech-output",
    "homey:manager:speech-input"
  ],
  "images": {
    "large": "/assets/images/large.png",
    "small": "/assets/images/small.png"
  },
  "author": {
    "name": "Sander Maas",
    "email": "sandermaas@gmail.com"
  },
  "bugs": {
    "url": "https://github.com/Retrospected/app.picnic/issues"
  },
  "flow": {
    "actions": [
      {
        "id": "add_product",
        "title": {
          "en": "Add a product",
          "nl": "Voeg een product toe"
        },
        "args": [
          {
            "name": "product",
            "type": "text",
            "placeholder": {
              "en": "bananas",
              "nl": "bananen"
            }
          }
        ]
      },
      {
        "id": "add_recipy",
        "title": {
          "en": "Add a recipy",
          "nl": "Voeg recept toe"
        },
        "args": [
          {
            "name": "recipy",
            "type": "autocomplete",
            "placeholder": {
              "en": "recipy",
              "nl": "recept"
            }
          }
        ]
      },
      {
        "id": "add_random_recipy",
        "title": {
          "en": "Add a random recipy",
          "nl": "Voeg willekeurig recept toe"
        }
      }
    ],
    "triggers": [
      {
        "id": "groceries_ordered",
        "title": {
          "en": "Groceries have been ordered",
          "nl": "Boodschappen zijn besteld"
        },
        "tokens": [
          {
            "name": "price",
            "type": "number",
            "title": {
              "en": "price",
              "nl": "prijs"
            },
            "example": 25
          },
          {
            "name": "eta_start",
            "type": "string",
            "title": {
              "en": "eta begin"
            },
            "example": "20:05"
          },
          {
            "name": "eta_end",
            "type": "string",
            "title": {
              "en": "eta end"
            },
            "example": "21:05"
          },
          {
            "name": "eta_date",
            "type": "string",
            "title": {
              "en": "eta date"
            },
            "example": "2018-09-03"
          }
        ]
      },
      {
        "id": "delivery_announced",
        "title": {
          "en": "Groceries will be delivered soon",
          "nl": "Boodschappen worden binnenkort geleverd"
        },
        "tokens": [
          {
            "name": "eta_start",
            "type": "string",
            "title": {
              "en": "eta begin"
            },
            "example": "20:25"
          },
          {
            "name": "eta_end",
            "type": "string",
            "title": {
              "en": "eta end"
            },
            "example": "20:45"
          },
          {
            "name": "eta_date",
            "type": "string",
            "title": {
              "en": "eta date"
            },
            "example": "2018-09-03"
          }
        ]
      },
      {
        "id": "groceries_delivered",
        "title": {
          "en": "Groceries have been delivered",
          "nl": "Boodschappen zijn bezorgd"
        },
        "tokens": [
          {
            "name": "delivery_time",
            "type": "string",
            "title": {
              "en": "delivery time",
              "nl": "aflevertijd"
            },
            "example": "20:45"
          },
          {
            "name": "delivery_date",
            "type": "string",
            "title": {
              "en": "eta_date",
              "nl": "afleverdatum"
            },
            "example": "2018-09-03"
          }
        ]
      }
    ]
  },
  "speech": {
    "en": {
      "element": {
        "addProduct": {
          "type": "regex",
          "value": "((add)?\\s?\\s?(a)?\\s?product)"
        }
      },
      "group": {
        "main": {
          "set": "addProduct",
          "ordered": false,
          "allowDisconnect": true,
          "capturingGroup": true
        }
      }
    },
    "nl": {
      "element": {
        "addProduct": {
          "type": "regex",
          "value": "((voeg)?\\s?\\s?(een)?\\s?product\\s?toe)"
        }
      }
    }
  },
  "speechExamples": {
    "en": [
      "Add a product."
    ],
    "nl": [
      "Voeg een product toe."
    ]
  }
}